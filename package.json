{
  "name": "@jsalazarv/vue-avatar",
  "version": "1.0.0",
  "description": "The avatar component allows you to display circular or square user profile images.",
  "main": "dist/v-avatar.ssr.js",
  "browser": "dist/v-avatar.esm.js",
  "module": "dist/v-avatar.esm.js",
  "unpkg": "dist/v-avatar.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsalazarv/jsalazarv-vue-avatar/tree/dev"
  },
  "homepage": "https://github.com/jsalazarv/jsalazarv-vue-avatar/tree/dev",
  "license": "MIT",
  "author": "Juan Salazar <jsalazarv8@gmail.com>",
  "keywords": [
    "avatar",
    "avatar-component",
    "avatar component",
    "vue-avatar-component",
    "vue avatar component",
    "vue-avatar",
    "vue avatar",
    "vue-component",
    "vue component",
    "v-avatar",
    "vue",
    "jsalazarv"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^10.2.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
